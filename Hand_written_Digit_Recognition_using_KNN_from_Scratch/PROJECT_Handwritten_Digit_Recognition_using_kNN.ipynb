      "source": [
        "\n",
        "def distance_weighted_knn(train_X, train_Y, test_X, n, k):\n",
        "  unique_class_labels = np.unique(train_Y)\n",
        "\n",
        "  weights = np.zeros((test_X.shape[0], unique_class_labels.shape[0]))\n",
        "  for test_idx, test_input in enumerate(test_X):\n",
        "    indices_k, distances_k = k_nearest_neighbours(train_X, test_input, n, k)\n",
        "    labels_k = train_Y[indices_k]\n",
        "    for label_idx, each_label in enumerate(unique_class_labels):\n",
        "      label_weight = np.sum(np.where(labels_k == each_label, 1/distances_k, 0.0))\n",
        "      weights[test_idx][label_idx] = label_weight\n",
        "\n",
        "  highest_weight_indices = np.argmax(weights, axis=1)\n",
        "  predicted_test_Y = unique_class_labels[highest_weight_indices]\n",
        "  return predicted_test_Y"
      ],
      "execution_count": null,
      "outputs": []
    },
